---
layout: null
---
{
    "version": "https://jsonfeed.org/version/1",
    "title": "{{ site.title | xml_escape }}",
    "home_page_url": "{{ "/" | absolute_url }}",
    "feed_url": "{{ "/posts.json" | absolute_url }}",
    "description": {{ site.description | jsonify }},
    "icon": "{{ "/apple-touch-icon.png" | absolute_url }}",
    "favicon": "{{ "/favicon.ico" | absolute_url }}",
    "expired": false,
    "items": [
        {% for post in site.posts limit:100 %}
            {
                "type": "tvserie",
                "title": {{ post.title | jsonify }},
                "overview": {{ post.overview | jsonify }},
                "img_path": {{ post.img_path | jsonify }},
                "tmdb_id": {{ post.tmdb_id | jsonify }},
                "imdb_id": {{ post.imdb_id | jsonify }},
                "runtime": {{ post.runtime | jsonify }},
                "release_date": {{ post.release_date | jsonify }},
                "genres": [
                    {% for genre in post.genres %}{{ genre | jsonify }}{% endfor %}
                ],

                "genres": [{% for genre in post.genres %}
                        {{ genre | jsonify }}
                    {% unless forloop.last %},{% endunless %}{% endfor %}
                ],

                "casts": [
                    {% for cast in post.casts %}
                        {{ cast | jsonify }}
                    {% endfor %}
                ],
                "crews": [
                    {% for crew in post.crews %}
                        {{ crew | jsonify }}
                    {% endfor %}
                ],
                "trailer": {{ post.trailer | jsonify }},
                "certification": {{ post.certification | jsonify }},
                "adult": {{ post.adult | jsonify }},
                "vote_average": {{ post.vote_average | jsonify }},
                "vote_count": {{ post.vote_count | jsonify }},
                "qualitys": [
                    {% for quality in post.qualitys %}
                        {{ quality | jsonify }}
                    {% endfor %}
                ],
                "audios": [
                    {% for audio in post.audios %}
                        {{ audio | jsonify }}
                    {% endfor %}
                ],
                "extensions": [
                    {% for extension in post.extensions %}
                        {{ extension | jsonify }}
                    {% endfor %}
                ]

            }{% if forloop.last == false %},{% endif %}
        {% endfor %}
        ]
    }