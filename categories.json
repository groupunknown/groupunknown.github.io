---
layout: null
---
{
    "genres": [{% for items in site.data.filter.genres %}{% assign posts = site.posts | where: "genres", items.item %}
        {
            "item": {{ items.item | jsonify }},
            "count": {{ posts.size }}
        }{% if forloop.last == false %},{% endif %}{% endfor %}
    ],
    "parents_guide": [{% for items in site.data.filter.certification %}{% assign posts = site.posts | where: "certification", items.item %}
        {
            "item": {{ items.item | jsonify }},
            "count": {{ posts.size }}
        }{% if forloop.last == false %},{% endif %}{% endfor %}
    ]
}
"vote_average": [


{% for post in site.posts %}
    {% for i in (10..20) %}
        {% if post.vote_average | remove: "." == i %}
            {% capture rating_a_count %}{% increment rating_a_count %}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% for i in (21..40) %}
        {% if post.vote_average | remove: "." == i %}
            {% capture rating_b_count %}{% increment rating_b_count %}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% for i in (41..60) %}
        {% if post.vote_average | remove: "." == i %}
            {% capture rating_c_count %}{% increment rating_c_count %}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% for i in (61..80) %}
        {% if post.vote_average | remove: "." == i %}
            {% capture rating_d_count %}{% increment rating_d_count %}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% for i in (81..100) %}
        {% if post.vote_average | remove: "." == i %}
            {% capture rating_e_count %}{% increment rating_e_count %}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% if post.vote_average | remove: "." == "00" %}
        {% capture rating_f_count %}{% increment rating_f_count %}{% endcapture %}
    {% endif %}
{% endfor %}
    {
        "item": "Péssimo",
        "count": {{ rating_a_count }}
    },
    {
        "item": "Ruim",
        "count": {{ rating_b_count }}
    },
    {
        "item": "Aceitável",
        "count": {{ rating_c_count }}
    },
    {
        "item": "Ótimo",
        "count": {{ rating_d_count }}
    },
    {
        "item": "Excelente",
        "count": {{ rating_e_count }}
    },
    {
        "item": "Nenhum",
        "count": {{ rating_f_count }}
    }
]