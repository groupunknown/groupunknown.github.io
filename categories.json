{
    "genres": [{% for items in site.data.filter.genres %}{% assign posts = site.posts | where: "genres", items.item %}
        {
            "item": {{ items.item | jsonify }},
            "count": {{ posts.size }}
        }{% if forloop.last == false %},{% endif %}{% endfor %}
    ],
    "parents_guide": [{% for items in site.data.filter.certification %}{% assign posts = site.posts | where: "certification", items.item %}
        {
            "item": {{ items.item | jsonify }},
            "count": {{ posts.size }}
        }{% if forloop.last == false %},{% endif %}{% endfor %}
    ]
}
"vote_average": [

{% assign rating_a_count = 0 %}
{% assign rating_b_count = 0 %}
{% assign rating_c_count = 0 %}
{% assign rating_d_count = 0 %}
{% assign rating_e_count = 0 %}
{% assign reting_f_count = 0 %}
{% for post in site.posts %}
    {% for i in (10..20) %}
        {% if i == post.vote_average | remove: "." %}
            {% capture rating_a_count %}{{ rating_a_count | plus: 1 }}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% for i in (21..40) %}
        {% if i contains post.vote_average | remove: "." %}
            {% capture rating_b_count %}{{ rating_b_count | plus: 1 }}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% for i in (41..60) %}
        {% if i contains post.vote_average | remove: "." %}
            {% capture rating_c_count %}{{ rating_c_count | plus: 1 }}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% for i in (61..80) %}
        {% if i contains post.vote_average | remove: "." %}
            {% capture rating_d_count %}{{ rating_d_count | plus: 1 }}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% for i in (81..100) %}
        {% if i contains post.vote_average | remove: "." %}
            {% capture rating_e_count %}{{ rating_e_count | plus: 1 }}{% endcapture %}
        {% endif %}
    {% endfor %}
    {% if post.vote_average | remove: "." == "00" %}
        {% capture rating_f_count %}{{ rating_f_count | plus: 1 }}{% endcapture %}
    {% endif %}
{% endfor %}
    {
        "item": "Péssimo",
        "count": {{ rating_a_count }}
    },
    {
        "item": "Ruim",
        "count": {{ rating_b_count }}
    },
    {
        "item": "Aceitável",
        "count": {{ rating_c_count }}
    },
    {
        "item": "Ótimo",
        "count": {{ rating_d_count }}
    },
    {
        "item": "Excelente",
        "count": {{ rating_e_count }}
    },
    {
        "item": "Nenhum",
        "count": {{ rating_f_count }}
    }
]

